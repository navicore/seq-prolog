include "src/term"
include "src/unify"
include "src/solve"

: test-compound ( -- )
  "=== Testing compound rename ===" io.write-line
  
  "X" 0 tvar tl1 "foo" swap tcompound
  "Input arg var ID: " over tcompound-args tlcar tvar-id int->string string.concat io.write-line
  
  100 rename-term
  
  "Counter after: " over int->string string.concat io.write-line
  drop
  "Output arg var ID: " swap tcompound-args tlcar tvar-id int->string string.concat io.write-line
;

: main ( -- ) test-compound ;
